    "@headlessui/react": "^1.4.2",
    "@heroicons/react": "^1.0.5",
    "@nivo/core": "^0.78.0",
    "@nivo/pie": "^0.78.0",
    "@react-spring/web": "^9.4.5",
    "axios": "^0.24.0",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "faker": "^5.5.3",
    "firebase": "^9.8.1",
    "pg": "^8.7.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.3.1",
    "react-redux": "^7.2.6",
    "react-rewards": "^1.1.2",
