{
  "name": "closet-manager",
  "version": "1.0.0",
  "description": "an app to manage your closet",
  "scripts": {
    "format": "prettier --write \"src/**/*.{js,jsx}\"",
    "lint": "eslint \"src/**/*.{js,jsx}\" --quiet",
    "dev": "parcel src/index.html",
    "build": "parcel build src/index.html",
    "start": "NODE_ENV=production node server && npm run build",
    "start:dev": "npm run dev & npm run start-server",
    "start:dev:logger": "LOGGING=true npm run start:dev",
    "start-server": "nodemon server -e html,js,scss --ignore src",
    "prepare": "husky install",
    "seed": "node script/seed.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.99.0",
